<model>
  <serverList>
    <% @server.each do |key, value| %>
    <server id="<%= key %>"><%= value %></server>
    <% end %>
  </serverList>
  <storageList>
    <% @storage.each do |key,value| %>
    <storage id="<%= key %>"><%= value %></storage>
    <% end %>
  </storageList>
  <switchList>
    <% @switch.each do |key,value| %>
    <switch id="<%= key %>"><%= value %></switch>
    <% end %>
  </switchList>
  <connectionList>
    <% @connection.each do |value| %>
    <connection>
      <from><%= value[:from] %></from>
      <over><%= value[:over] %></over>
      <to><%= value[:to] %></to>
    </connection>
    <% end %>
  </connectionList>
</model>